	--color-error-100: 255 209 204; /* ⬅ #ffd1cc */
	--color-error-200: 255 198 191; /* ⬅ #ffc6bf */
	--color-error-300: 254 163 153; /* ⬅ #fea399 */
	--color-error-400: 254 95 77; /* ⬅ #fe5f4d */
	--color-error-500: 253 26 0; /* ⬅ #fd1a00 */
