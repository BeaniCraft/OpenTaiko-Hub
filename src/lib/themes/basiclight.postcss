	--color-warning-200: 255 225 198; /* ⬅ #ffe1c6 */
	--color-warning-300: 255 207 163; /* ⬅ #ffcfa3 */
	--color-warning-400: 255 171 95; /* ⬅ #ffab5f */
	--color-warning-500: 255 135 26; /* ⬅ #ff871a */
	--color-warning-600: 230 122 23; /* ⬅ #e67a17 */
	--color-warning-700: 191 101 20; /* ⬅ #bf6514 */
	--color-warning-800: 153 81 16; /* ⬅ #995110 */
	--color-warning-900: 125 66 13; /* ⬅ #7d420d */
	/* error | #fd1a00 */
